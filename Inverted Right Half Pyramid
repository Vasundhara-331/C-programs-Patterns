//Inverted Right Half Pyramid
/*
* * * * *
* * * *
* * *
* *
*
*/
#include <stdio.h> 
int main() 
{ 
    int rows,i,j;
    printf("Enter the numbers of rows: ");
    scanf("%d",&rows);
    for (i = 0; i < rows; i++) 
        {
          for (j = 0; j < rows - i; j++)
        { 
            printf("* "); 
        } 
        printf("\n"); 
    } 
}
